filter(gl_est_etapa %in% aprobados |
gl_est_etapa %in% en_proceso |
gl_est_etapa %in% rechazados|
gl_est_etapa == 'EN EJECUCION'
& f_fin_proy <= '2021-03-31')  %>%
mutate(etapas_agrupadas =
